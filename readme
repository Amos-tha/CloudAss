# Either add "sudo" before all commands or use "sudo su" first
# Amazon Linux 2023

#!/bin/bash
sudo su
yum update -y
yum install httpd -y
yum install git -y
git clone --single-branch --branch Supervisor https://github.com/Amos-tha/CloudAss.git
cd CloudAss
yum install python-pip -y
pip3 install flask pymysql boto3
python3 app.py 